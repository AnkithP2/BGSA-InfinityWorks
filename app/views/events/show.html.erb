<p id="notice"><%= notice %></p>

<p>
  <strong>Event ID:</strong>
  <%= @event.id %>
    <br>
  <strong>Title:</strong>
  <%= @event.title %>
    <br>
  <strong>Start Time:</strong>
  <%= @event.starttime %>
    <br>
  <strong>End Time:</strong>
  <%= @event.endtime %>
    <br>
  <strong>Event Points:</strong>
  <%= @event.eventpoints %>
    <br>
  <% if @admin %>
    <strong>Login Code</strong>
    <%= @event.logincode %>
    <br> 

    <strong>User IDs RSVP:</strong>
      <% for user in @usersRSVP do %>
        <p>
        <%= user%> 
        </p>
      <% end %>

    <strong> User IDs Attended:</strong>
      <% for user in @usersAttend do %>
        <p>
        <%= user%> 
        </p>
      <% end %> 
  <%end%>
  

<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Delete', @event, method: :delete, data: { confirm: 'Are you sure?' } %> | 
<%= link_to 'Back', events_path %>
