<p id="notice"><%= notice %></p>

<h1>Events</h1>
<h2>Shows all events</h2>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Date</th>
      <th>Start Time</th>
      <th>End Time</th>
      <th>Login Code</th>
      <th>Location</th>
      <th>Event Points</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.date %></td>
        <td><%= event.starttime %></td>
        <td><%= event.endtime %></td>
        <td><%= event.logincode %></td>
        <td><%= event.location %></td>
        <td><%= event.eventpoints %></td>
        <td><%= link_to 'Show', event_path(event) %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
  </tbody>
</table>

